    "apollo-server-cloud-functions": "^2.11.0",
    "apollo-server-express": "^2.11.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.3.0",
    "graphql": "^14.6.0",
    "parcel-bundler": "^1.12.4",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
